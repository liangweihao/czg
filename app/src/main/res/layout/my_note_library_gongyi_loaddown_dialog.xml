<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dimen_10dp"
            android:gravity="right"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_note_library_gongyi_loaddown_delete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/diaglo_delete" />

            <View
                android:id="@+id/view_note_library_gongyi_loaddown"
                android:layout_width="@dimen/dimen_1dp"
                android:layout_height="@dimen/dimen_43dp"
                android:layout_marginRight="@dimen/dimen_13dp"
                android:background="@color/white" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_my_project_manager_diaglo_bg"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@mipmap/diaglo_bg"
                android:gravity="center"
                android:paddingBottom="@dimen/dimen_28dp"
                android:paddingTop="@dimen/dimen_28dp"
                android:text="材料与施工工艺"
                android:textColor="@color/white"
                android:textSize="@dimen/dimen_20sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_10dp"
                android:layout_marginLeft="@dimen/dimen_25dp"
                android:layout_marginTop="@dimen/dimen_15dp"
                android:text="节点介绍："
                android:textColor="#6e6e6e"
                android:textSize="@dimen/dimen_14sp" />


            <TextView
                android:id="@+id/tv_note_library_gongyi_loaddown_jiedian_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_20dp"
                android:layout_marginLeft="@dimen/dimen_25dp"
                android:layout_marginRight="@dimen/dimen_25dp"
                android:text="结构专业节点，结构专业节点，结构专业节点，结构专业节点，结构专业节点，结构专业节点"
                android:textColor="#6e6e6e"
                android:textSize="@dimen/dimen_12sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_13dp"
                android:layout_marginRight="@dimen/dimen_13dp"
                android:background="@drawable/shape_note_library_loaddown_bg"
                android:orientation="vertical"
                android:paddingBottom="@dimen/dimen_15dp"
                android:paddingLeft="@dimen/dimen_8dp"
                android:paddingRight="@dimen/dimen_8dp"
                android:paddingTop="@dimen/dimen_15dp">

                <TextView
                    android:id="@+id/tv_note_library_gongyi_loaddown_liucheng"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="一、施工工艺流程"
                    android:textColor="#6e6e6e"
                    android:textSize="@dimen/dimen_14sp" />

                <TextView
                    android:id="@+id/tv_note_library_gongyi_loaddown_liucheng_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="         弹线——>安装跳杆、弹线——>安装跳杆弹线——>安装跳杆弹线——>安装跳杆"
                    android:textColor="#6e6e6e"
                    android:textSize="@dimen/dimen_12sp" />

                <TextView
                    android:id="@+id/tv_note_library_gongyi_loaddown_cuoshi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="二、主要施工技术措施"
                    android:textColor="#6e6e6e"
                    android:textSize="@dimen/dimen_14sp" />

                <TextView
                    android:id="@+id/tv_note_library_gongyi_loaddown_cuoshi_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="         弹线——>安装跳杆、弹线——>安装跳杆弹线——>安装跳杆弹线——>安装跳杆"
                    android:textColor="#6e6e6e"
                    android:textSize="@dimen/dimen_12sp" />

            </LinearLayout>


            <TextView
                android:id="@+id/btn_note_library_gongyi_loaddown"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_25dp"
                android:layout_marginLeft="@dimen/dimen_34dp"
                android:layout_marginRight="@dimen/dimen_34dp"
                android:layout_marginTop="@dimen/dimen_25dp"
                android:gravity="center"
                android:background="@drawable/shape_login_bg"
                android:paddingBottom="@dimen/dimen_13dp"
                android:paddingTop="@dimen/dimen_13dp"
                android:text="下载施工工艺"
                android:textColor="@color/white"
                android:textSize="@dimen/dimen_15sp" />

        </LinearLayout>

    </LinearLayout>

</FrameLayout>